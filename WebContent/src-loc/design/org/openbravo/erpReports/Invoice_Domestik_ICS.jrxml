	<queryString>
		<![CDATA[select DISTINCT
	i.ad_org_id,
	i.documentno,
	i.docstatus as status,
	il.Aumqty,
upper(format_terbilang(i.grandtotal)) as Says,

(select
o.linenetamt as DPP  from c_invoiceline o
left join c_invoice z on  z.c_invoice_id=o.c_invoice_id
left join m_product p on p.m_product_id=o.m_product_id
where z.c_invoice_id = $P{DOCUMENT_ID}
and p.isstocked='N')as DPP,

(select
case when o.LineNetAmt >0 then 'Biaya Kirim' else '' end as biaya_kirim
from c_invoiceline o
left join c_invoice z on  z.c_invoice_id=o.c_invoice_id
left join m_product p on p.m_product_id=o.m_product_id
where z.c_invoice_id = $P{DOCUMENT_ID}
and p.isstocked='N')as DPP_Kirim,




ol.description as orderdescription,
io.documentno as docshipment,
i.GrandTotal,
il.LineNetAmt,
ci.taxamt,
p.name as productname,
	i.description as remark,
	io.documentno,
	il.PriceActual,
iol.description as minoutdes,
il.line,
l.city,
l.postal,
io.em_oez_depature,

case when i.c_doctype_id='0' then 'Jl. Raya Waru No.30 A, Waru, Sidoarjo 61256, Jawa Timur, Indonesia'
when i.c_doctype_id='CDC8887EFEB94283A63B40F1BC12D065' then 'Jl. Raya Waru No.30 A, Waru, Sidoarjo 61256, Jawa Timur, Indonesia'

when i.c_doctype_id='1D057B9B071A42A99D180608E35961EA' then 'Jl. Raya Waru No.30 A, Waru, Sidoarjo 61256, Jawa Timur, Indonesia'

when i.c_doctype_id='2E1572548F5A47FB83E6DF83A89A4F62' then 'Jl. Raya Waru No.30 A, Waru, Sidoarjo 61256, Jawa Timur, Indonesia'

when i.c_doctype_id='9C99669DB5D64FB6B4097422FABE38C6' then 'Jl. Raya Waru No.30 A, Waru, Sidoarjo 61256, Jawa Timur, Indonesia'

else '' end as orgaddres,


p.weight,
p.em_oez_gross_weight,
p.weight * il.qtyinvoiced as netweight,
p.em_oez_gross_weight * il.qtyinvoiced as groswight,

--io.em_oez_container_no as containerno,
--io.em_oez_seal_no as sealno,

k.loading_port_etd,
k.dest_port_eta,
k.dest_port_city,
k.loading_port_city,
k.Voyage_Number,
k.Vessel_Name,


io.MovementDate as depature,
io.em_oez_sealno,
io.em_oez_containerno,
io.em_oez_vessel_name,
io.em_oez_voyage_number,
io.em_oez_dest_port,
io.EM_Oez_Letterofcredit_date,
io.em_oez_letterofcredit_number,
io.EM_Oez_Port_Of_Loading,
EM_Oez_Port_Of_Discharge,
iol.description as linedes,
pc.description as productcategorydes,
ol.aumqty,
i.EM_Oez_Invoice_Notes,
p.em_oez_packaging_size,
CASE WHEN p.em_oez_packaging_size ='em_oez_size1' then '1 kg x 10'
when p.em_oez_packaging_size = 'em_oez_size2' then '900 gr x 2 x 6'
when p.em_oez_packaging_size = 'em_oez_size3' then '450 gr'
when p.em_oez_packaging_size = 'em_oez_size4' then '1,8 kg x 6'
when p.em_oez_packaging_size = 'em_oez_size5' then '10p x 2'
when p.em_oez_packaging_size = 'em_oez_size6' then '1,6 kg x 6'
when p.em_oez_packaging_size = 'em_oez_size7' then '1,8 kg x 10'

else '' end as packagingsize,

p.weight*il.QtyInvoiced as totalaumqty,

CASE WHEN il.C_aum ='72BA247D31F745F3AF11F74A5E2CCBEF' then 'KG'
when il.C_aum = 'E26A062373F54C539E68D1DB76F301B6' then 'TY'
when il.C_aum ='4028E6C72959682B01295ADC1A380221' then 'BA'
when il.C_aum = '52DFFEBF92F343AFBAE745EE30421AA1' then 'PH'
else '' end as AUMCODE,
il.aumqty,

z.name as incotermname,

p.em_oez_packaging,
CASE WHEN p.em_oez_packaging ='em_oez_vacuum_pack' then 'Vacuum Pack'
when p.em_oez_packaging = 'em_oez_pollybag' then 'Polybag'
when p.em_oez_packaging = 'em_oez_tray' then 'Tray'
else '' end as packaging,


p.em_oez_frozen_type,
CASE WHEN p.em_oez_frozen_type ='em_oez_semi_iqf' then 'Semi IQF'
when p.em_oez_frozen_type = 'em_oez_iqf' then 'IQF'
when p.em_oez_frozen_type = 'em_oez_block_frozen' then 'Block Frozen'
when p.em_oez_frozen_type = 'em_oez_semi_block' then 'Semi Block'
else '' end as frozentype,

p.em_oez_raw_type,
CASE WHEN p.em_oez_raw_type ='em_oez_vanamei' then 'Vannamei'

else '' end as rawtype,

p.em_oez_type_of_goods,

CASE WHEN p.em_oez_type_of_goods ='em_oez_raw' then 'RAW'
when p.em_oez_type_of_goods = 'em_oez_cooked' then 'Cooked'
when p.em_oez_type_of_goods = 'em_oez_blanched_cut' then 'Blanched Cut Octopus Natural'
when p.em_oez_type_of_goods = 'em_oez_boiled_cut' then 'Prima Octopus Treated'
when p.em_oez_type_of_goods = 'em_oez_cooked_natural' then 'Cooked Natural'
when p.em_oez_type_of_goods = 'em_oez_Cooked_treated' then 'Cooked Treated'
when p.em_oez_type_of_goods = 'em_oez_raw_natural' then 'Prima Green Natural'
when p.em_oez_type_of_goods = 'em_oez_raw_treated' then 'Prima Red Treated'

when p.em_oez_type_of_goods = 'em_oez_prima_blue_treated_iqf' then 'Prima Blue Treated IQF'
when p.em_oez_type_of_goods = 'em_oez_prima_red_treated' then 'Prima Red Treated'
when p.em_oez_type_of_goods = 'em_oez_prima_green_natural' then 'Prima Green Natural'
when p.em_oez_type_of_goods = 'em_oez_prima_red_treated_iqf' then 'Prima Red Treated IQF'

else '' end as typeofgoods,

ol.em_oez_aum_priceactual,

p.name2,
	bp.name as customer,
	l.address1||coalesce(' '||l.address2,'') as address1,
	coalesce(l.city,'')||coalesce(' '||l.postal,'')||coalesce(' '||r.name,'')||coalesce(' '||c.name,'') as address2,
	coalesce('Tel. '||bpl.phone,'')||coalesce(' Fax. '||bpl.fax,'') as phonefax,
	u.name as customer_contact,
	io.em_oez_codocumentno,
	ad_org.name,
	ad_orginfo.taxid as tax,
	org_loc.ADDRESS1 AS ORG_ADDRESS1,
	org_loc.ADDRESS2 AS ORG_ADDRESS2,
	org_loc.POSTAL AS ORG_POSTAL,
	org_loc.C_COUNTRY_ID AS COUNTRY_ID,
	i.dateinvoiced as invoice_date,
	cr.iso_code as currency_code,
	pt.name as payment_term,
	pt.description as payment_term_description,
	il.line as invoice_line,
	p.name as product,
	p.value as partcode,
	um.x12de355 as uom,
	mb.name as brand,
	ba.bank_name,
	ba.accountno,
	um.name as uom_name,
	il.qtyinvoiced as invoice_qty,
CASE WHEN i.docstatus = 'CO' THEN 'gunttdnew.png' ELSE 'blank.png' end as ttd,
CASE WHEN i.docstatus = 'CO' THEN 'istana.png' ELSE 'blank.png' end as ttd2,


	ol.pricelist as orderpricelist,
	il.priceactual as unit_price,
	il.linenetamt as line_net_amount,
	coalesce(p.description, '') || coalesce(il.description, '') as product_description,
	(select min (duedate) from FIN_Payment_Schedule where c_invoice_id=i.c_invoice_id) as invoice_line_due_date,
	coalesce(io.documentno,'') as shipment_no,
	io.movementdate::date as delivery_date,
	coalesce(o.poreference,'') as order_reference,
	i.totallines as total_invoice_amount
from c_invoice i
left join c_invoiceline il on i.c_invoice_id=il.c_invoice_id
left join ad_org on ad_org.ad_org_id=i.ad_org_id
left join ad_orginfo on AD_ORGINFO.ad_org_id=i.ad_org_id


--left join c_location org_loc on ad_orginfo.c_location_id=org_loc.c_location_id
left join c_location on c_location.ad_org_id=i.ad_org_id
left join c_bpartner bp on i.c_bpartner_id = bp.c_bpartner_id
left join c_bpartner_location bpl on i.c_bpartner_location_id = bpl.c_bpartner_location_id
left join c_location l on bpl.c_location_id = l.c_location_id

left join C_LOCATION org_loc on AD_ORGINFO.C_LOCATION_ID =org_loc.C_LOCATION_ID

left join c_region r on l.c_region_id = r.c_region_id
left join c_country c on l.c_country_id = c.c_country_id
left join ad_user u on u.ad_user_id=i.ad_user_id
left join c_currency cr on i.c_currency_id = cr.c_currency_id
left join c_paymentterm pt on i.c_paymentterm_id = pt.c_paymentterm_id
left join m_inoutline iol on il.m_inoutline_id = iol.m_inoutline_id
left join m_inout io on io.m_inout_id= iol.m_inout_id
left join c_orderline ol on ol.c_orderline_id=il.c_orderline_id
left join c_order o on o.c_order_id=ol.c_order_id
left join c_incoterms z on z.c_incoterms_id=o.c_incoterms_id
left join m_product p on il.m_product_id = p.m_product_id
left join c_uom um on il.c_uom_id = um.c_uom_id
left join C_BP_BankAccount ba on bp.c_bpartner_id=ba.c_bpartner_id
left join m_brand mb on mb.m_brand_id=p.m_brand_id
left join m_product_category pc on p.m_product_category_id=pc.m_product_category_id
left join oez_shipmentdata k on k.m_inout_id=io.m_inout_id

left join c_tax ct on ct.c_tax_id=il.c_tax_id
left join c_invoicelinetax ci on ci.c_invoiceline_id=il.c_invoiceline_id

--left join c_invoicetax cit on cit.c_invoice_id=i.c_invoice_id
where i.c_invoice_id=$P{DOCUMENT_ID}
--and cit.C_Tax_ID='E9E03B426CC74DD396F958E0E4BF535F'
and p.IsStocked='Y'


--Sample Invoice General product = "55F4807A53BF489A9F0FD612BDB2967E"
--sample Invoice status DRAFT = "388E524BA7284EB888C6A11E2A95BA0F"
order by il.line asc]]>
