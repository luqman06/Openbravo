	<queryString>
		<![CDATA[SELECT p.documentno as PAYMENTNO,
org.NAME AS ORG_NAME,
bp.name AS BUSINESSPARTNER,
p.paymentdate as PAYMENT_DATE,
p.amount as TOTAL,
bpl.name as bplocation,
c.address1,
fa.name as financialaccountname,
pm.name as PAYMENT_METHOD,
ORG_DATA_INFO.name as org_country_header,
ORG_DATA_INFO.description as org_website, --ambil dari contact di organization
ORG_DATA_INFO.phone as org_phone, --ambil dari contact di organization
ORG_DATA_INFO.phone2 as org_phone_al, --ambil dari contact di organization
ORG_DATA_INFO.fax as org_fax, --ambil dari contact di organization
ORG_DATA_INFO.city as org_city, --ambil di city contact di organization
ORG_DATA_INFO.supervisor as org_supervisor,
ORG_DATA_INFO.title as org_title,
C_LOCATION_DESCRIPTION(AD_ORGINFO.C_LOCATION_ID) AS LOCATION,
bank.bank_name as bank_name,
bank.a_name as bank_an,
bank.accountno as no_rek,
cc.name as costcenter,
cur.cursymbol as CURRENCY, p.isreceipt as ISRECEIPT,
       org.ad_org_id as legalentity, org.name as LE_NAME, ad_orginfo.taxid as LE_TAXID,
       c_location_description(ad_orginfo.c_location_id) as LE_LOCATION
FROM fin_payment p left join c_bpartner bp on (p.c_bpartner_id=bp.c_bpartner_id)
     left join c_bpartner_location bpl on (bpl.c_bpartner_id=bp.c_bpartner_id)
  left join c_location c on (c.c_location_id=bpl.c_location_id)

     join c_currency cur on (p.c_currency_id=cur.c_currency_id)
     left outer join fin_paymentmethod pm on (p.fin_paymentmethod_id=pm.fin_paymentmethod_id)
 	left outer join C_BP_BankAccount bank on bp.c_bpartner_id=bank.c_bpartner_id
left join fin_financial_account fa on fa.fin_financial_account_id=p.fin_financial_account_id

left outer join c_costcenter cc on p.c_costcenter_id=cc.c_costcenter_id, ad_org org,
 (select d.NAME,e.description,e.email,e.phone,e.phone2,e.fax,c.city,f.name as supervisor,e.title
     from fin_payment a left join AD_ORGINFO b on a.ad_org_id=b.ad_org_id
     left join c_location c on b.c_location_id=c.c_location_id
     left join c_country d on c.c_country_id=d.c_country_id
     left join ad_user e on b.ad_user_id=e.ad_user_id

	 left join ad_user f on e.supervisor_id=f.ad_user_id
     where a.fin_payment_id = $P{DOCUMENT_ID}) as ORG_DATA_INFO,
ad_orginfo
WHERE p.fin_payment_id=$P{DOCUMENT_ID}
      and org.ad_org_id = ad_orginfo.ad_org_id
      and org.ad_org_id = (select o.ad_org_id
                           from ad_org o join ad_orgtype t on (o.ad_orgtype_id=t.ad_orgtype_id)
                           where ad_isorgincluded(p.ad_org_id, o.ad_org_id, p.ad_client_id)<>-1
                                 and (t.islegalentity='Y' OR t.isacctlegalentity='Y'))]]>
	</queryString>
