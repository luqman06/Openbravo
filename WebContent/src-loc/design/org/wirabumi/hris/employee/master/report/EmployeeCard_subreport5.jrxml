	<queryString><![CDATA[select  Name,  Email,  Phone,  phone2 as "Alternative Phone",  BirthDay,  
(case when em_hris_isfamily='Y' then 'Yes' else 'No' end) as Family,
(case when em_hris_istertanggung='Y' then 'Yes' else 'No' end) as Insured,  
em_hris_relation as Relation,
(select name from ad_ref_list rl where rl.ad_reference_id=
(select ad_reference_id from ad_reference where name='Marital Status')
and rl.value=em_hris_maritalstatus) as "Marital Status"
from ad_user 
where c_bpartner_id=$P{C_BPartner_ID}]]></queryString>

