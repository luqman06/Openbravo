	<queryString><![CDATA[select bp.name as employee, jt.name as jobname, tr.name as training
from c_bpartner bp
inner join hris_jobtitle jt on jt.hris_jobtitle_id=bp.em_hris_jobtitle_id
left join hris_competency_jobtitle cpjt on cpjt.hris_jobtitle_id=jt.hris_jobtitle_id
left join hris_competency cp on cp.hris_competency_id=cpjt.hris_competency_id
left join hris_training_competency trcp on trcp.hris_competency_id=cp.hris_competency_id
left join hris_training tr on tr.hris_training_id=trcp.hris_training_id
where bp.ad_client_id = $P{ad_client_id}
 and bp.isemployee = 'Y'
 and cpjt.hris_competency_id not in (select hris_competency_id from hris_c_bp_competency where c_bpartner_id=bp.c_bpartner_id)
order by bp.name asc]]></queryString>

