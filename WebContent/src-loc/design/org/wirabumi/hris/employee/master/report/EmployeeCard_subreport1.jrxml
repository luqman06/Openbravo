	<queryString><![CDATA[select  
(select name from ad_ref_list rl where rl.ad_reference_id=
(select ad_reference_id from ad_reference where name='Employment Type')
and rl.value=info.Employementtype) as "Employment Type", info.contractno as "Contract No",
info.validfrom as "Valid From", info.validto as "Valid To", dep.name as Department,
info.level as Level, (select name from ad_ref_list rl where rl.ad_reference_id=
(select ad_reference_id from ad_reference where name='Employee Position')
and rl.value=info.Position ) as Position, 
(select name from hris_jobtitle where hris_jobtitle_id=info.hris_jobtitle_id) as "Job Title",
(select name from c_bpartner where c_bpartner_id=info.report_to and IsEmployee='Y') as "Report To"
from HRIS_C_Bp_Empinfo  info    
left join HRIS_C_Bp_Department dep on dep.HRIS_C_Bp_Department_id = info.hris_c_bp_department_id
where info.c_bpartner_id=$P{C_BPartner_ID}
order by info.validto desc]]></queryString>

