	<queryString><![CDATA[select (select name from ad_ref_list rl where rl.ad_reference_id=
(select ad_reference_id from ad_reference where name='Education Level')
and rl.value=edu.level) as level, Level, edu.Dicipline, edu.Institution_Name, edu.validfrom as "Starting Date", 
edu.validto as "Ending Date", edu.gradenote as "Grade Note", edu.Grade,
(case when edu.Graduate='Y' then 'Yes' else 'No' end) as Graduate
from HRIS_C_Bp_education edu
where c_bpartner_id=$P{C_BPartner_ID}
order by edu.level asc, edu.validfrom desc, edu.validto desc, edu.institution_name asc
]]></queryString>

