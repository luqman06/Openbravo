	<parameter name="c_bpartner_id" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String("")]]></defaultValueExpression>
	</parameter>
	<parameter name="hris_jobtitle_id" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String("")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select j.name as jobname, c.name as competency
	,(case when exists (select 1 from hris_c_bp_competency bpc
					inner join c_bpartner bp on bp.c_bpartner_id=bpc.c_bpartner_id
		where bp.c_bpartner_id=$P{c_bpartner_id}
		and bpc.hris_competency_id=c.hris_competency_id)
			then 'Yes' else 'No' end) as certified	
	,(case when exists (select 1 from hris_c_bp_competency bpc
					inner join c_bpartner bp on bp.c_bpartner_id=bpc.c_bpartner_id
		where bp.c_bpartner_id=$P{c_bpartner_id}
		and bpc.hris_competency_id=c.hris_competency_id
		and bpc.validto<now())
			then 'Yes' else 'No' end) as expired,
(select name from c_bpartner where c_bpartner_id=$P{c_bpartner_id}) employee,
(select name from hris_jobtitle where hris_jobtitle_id = $P{hris_jobtitle_id}) as job_title
from hris_jobtitle j
inner join hris_competency_jobtitle cj on cj.hris_jobtitle_id=j.hris_jobtitle_id
inner join hris_competency c on cj.hris_competency_id=c.hris_competency_id
where j.hris_jobtitle_id = $P{hris_jobtitle_id}
order by j.name asc, c.name asc]]></queryString>

