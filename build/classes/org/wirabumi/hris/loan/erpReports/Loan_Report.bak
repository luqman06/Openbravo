	<queryString><![CDATA[select bp.name as employee, bp.value as NIK, (select name from ad_ref_list rl where rl.ad_reference_id=
(select ad_reference_id from ad_reference where name='Employee Position')
and rl.value=bp.em_hris_position) as Position, bpd.name as department,  trunc(ln.Amount) as hutang, trunc(interest) as bunga, trunc(ln.amount+(ln.amount*interest/100)) as "Hutang+Bunga", duration as "Jangka Waktu",  
sped.amount as potongan, totalpaid as "Total Yang Dibayar", sp.name as Keterangan
from ln_loan ln
inner join c_bpartner bp on bp.c_bpartner_id=ln.c_bpartner_id
left join hris_c_bp_department bpd on bpd.hris_c_bp_department_id=bp.em_hris_c_bp_department_id
left join pyr_sp_employee spe on spe.c_bpartner_id=ln.c_bpartner_id
left join pyr_spe_deduction sped on sped.pyr_sp_employee_id=spe.pyr_sp_employee_id
left join pyr_salarypayment sp on sp.pyr_salarypayment_id=spe.pyr_salarypayment_id
where sped.value='CP'
and sp.valutadate>=datefrom
and sp.valutadate<=dateto
and ln_loan_id=$P{ln_loan_id}]]></queryString>

