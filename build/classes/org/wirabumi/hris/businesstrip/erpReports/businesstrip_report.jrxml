	<queryString><![CDATA[select bp.name as Employee, bp.value as NIK, dp.name as "Unit Kerja",
(select name from ad_ref_list rl where rl.ad_reference_id=
(select ad_reference_id from ad_reference where name='Employee Position')
and rl.value=bp.em_hris_position) as Jabatan, bt.destination, bt.achieved,
bt.datefrom, bt.dateto, bt.dateaction, bt.name, bpl.phone as phone_no,
org.name organization, bl.phone, org.social_name as info, 
cl.address1 || ', ' || cl.city || ', ' || cl.postal || ', ' || cn.name as address, 
i.binarydata as imageblob, bt.docstatus, bt.description, bt.result, bt.objective
from bt_businesstrip bt
inner join ad_org org on org.ad_org_id = bt.ad_org_id
left join ad_orginfo oi on oi.ad_org_id = bt.ad_org_id
left join c_bpartner ip on ip.c_bpartner_id = oi.c_bpartner_id
left join c_bpartner_location bl on bl.c_bpartner_id = oi.c_bpartner_id
left join c_location cl on cl.c_location_id = oi.c_location_id
left join c_country cn on cn.c_country_id = cl.c_country_id
left join ad_image i on i.ad_image_id = oi.your_company_document_image
inner join c_bpartner bp on bp.c_bpartner_id=bt.c_bpartner_id
left join c_bpartner_location bpl on bpl.c_bpartner_id=bt.c_bpartner_id
left join hris_c_bp_department dp on dp.hris_c_bp_department_id=bp.em_hris_c_bp_department_id
left join c_year yr on yr.ad_client_id=bt.ad_client_id
where bt.bt_businesstrip_id=$P{bt_businesstrip_id}]]></queryString>

