	<queryString><![CDATA[select loc.Address1, loc.Address2, loc.city as "City Name", loc.postal as "Postal Code", 
reg.description as Region, coun.name as Country, bloc.Phone, bloc.phone2 as "Alternative Phone",
(case when em_hris_iscurrentaddr='Y' then 'Yes' else 'No' end) as "Current Address"
from C_BPartner_Location bloc
left join c_location loc on loc.c_location_id=bloc.c_location_id
left join c_country coun on coun.c_country_id=loc.c_country_id
left join c_region reg on reg.c_region_id=loc.c_region_id
where c_bpartner_id=$P{C_BPartner_ID}]]></queryString>

