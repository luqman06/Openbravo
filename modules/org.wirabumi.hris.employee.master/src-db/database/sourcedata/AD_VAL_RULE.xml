<!--068F760318114F7FA93C29DBBAE862D1-->  <AD_CLIENT_ID><![CDATA[0]]></AD_CLIENT_ID>
<!--068F760318114F7FA93C29DBBAE862D1-->  <AD_ORG_ID><![CDATA[0]]></AD_ORG_ID>
<!--068F760318114F7FA93C29DBBAE862D1-->  <ISACTIVE><![CDATA[Y]]></ISACTIVE>
<!--068F760318114F7FA93C29DBBAE862D1-->  <NAME><![CDATA[Jenjang Jabatan Mutasi]]></NAME>
<!--068F760318114F7FA93C29DBBAE862D1-->  <TYPE><![CDATA[S]]></TYPE>
<!--068F760318114F7FA93C29DBBAE862D1-->  <CODE><![CDATA[ad_ref_list.value in (select A.value from
((select rl2.value,rl2.name,rl2.seqno
from ad_ref_list rl1, ad_ref_list rl2
where rl1.ad_reference_id=rl2.ad_reference_id
and rl1.value=@Fromechelon@ and rl1.ad_reference_id='7FEDB3A24E1A426986F8E1D6DA68832C'
and rl1.seqno>=rl2.seqno
and rl2.isactive='Y'
order by rl2.seqno desc
limit 3)
union
(select rl2.value,rl2.name,rl2.seqno
from ad_ref_list rl1, ad_ref_list rl2
where rl1.ad_reference_id=rl2.ad_reference_id
and rl1.value=@Fromechelon@ and rl1.ad_reference_id='7FEDB3A24E1A426986F8E1D6DA68832C'
and rl1.seqno<rl2.seqno
and rl2.isactive='Y'
order by rl2.seqno asc)) A
order by A.seqno desc)]]></CODE>
<!--068F760318114F7FA93C29DBBAE862D1-->  <AD_MODULE_ID><![CDATA[D5FD35D6C7604921BA951B7E0EEE5DC2]]></AD_MODULE_ID>
