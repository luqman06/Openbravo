	<parameter name="ta_akumulasi_pot_gaji_id" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[select org.name as "Org", cl.city as "KOTA", bp.value as "NO. PEG", bp.name as "NAMA", 
pot.name as "Keterangan", dep.name as "Department",
 (select name from ad_ref_list where ad_reference_id=
 (select ad_reference_id from ad_reference where name='Employee Position') 
 and value=tit.position)||'-'|| tit.name as "Jabatan", amount,
 startdate, enddate, monthday as duration, pot.ta_akumulasi_pot_gaji_id
from ta_akumulasi_pot_gaji pot
left join c_bpartner bp on bp.c_bpartner_id=pot.c_bpartner_id
left join hris_jobtitle tit on tit.hris_jobtitle_id = bp.em_hris_jobtitle_id
left join hris_c_bp_department dep on dep.hris_c_bp_department_id = bp.em_hris_c_bp_department_id
left join ad_org org on org.ad_org_id=pot.ad_org_id
left join ad_orginfo oi on oi.ad_org_id = pot.ad_org_id
left join c_location cl on cl.c_location_id=oi.c_location_id
where ta_akumulasi_pot_gaji_id=$P{ta_akumulasi_pot_gaji_id}]]></queryString>

