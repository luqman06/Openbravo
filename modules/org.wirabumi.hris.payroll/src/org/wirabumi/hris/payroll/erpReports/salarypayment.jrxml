	<subDataset name="Employee" >

		<parameter name="pyr_salarypayment_id" isForPrompting="false" class="java.lang.String"/>
		<queryString><![CDATA[select a.*  from (select round((select sum(spee.amount) from pyr_spe_earning spee where spee.pyr_sp_employee_id=spe.pyr_sp_employee_id)-
(select sum(sped.amount) from pyr_spe_deduction sped where sped.pyr_sp_employee_id=spe.pyr_sp_employee_id), 2) as total_amount,
(case when spee.amount>0 then bp.name end)as customer, (case when spee.amount>0 then 'Earning' end) as keterangan, 
(case when spee.amount>0 then pe.name end) as name, (case when spee.amount>0 then round(spee.amount, 2) end) as total 
from c_bpartner bp
inner join pyr_sp_employee spe on spe.c_bpartner_id = bp.c_bpartner_id
inner join pyr_spe_earning spee  on  spee.pyr_sp_employee_id = spe.pyr_sp_employee_id
inner join pyr_earning pe on pe.pyr_earning_id=spee.pyr_earning_id
where bp.isemployee = 'Y'
and spe.pyr_salarypayment_id=$P{pyr_salarypayment_id}
order by spee.amount desc) a
where a.* is not null
union
select b.* from 
(select round((select sum(spee.amount) from pyr_spe_earning spee where spee.pyr_sp_employee_id=spe.pyr_sp_employee_id)-
(select sum(sped.amount) from pyr_spe_deduction sped where sped.pyr_sp_employee_id=spe.pyr_sp_employee_id), 2) as total_amount,
(case when sped.amount>0 then bp.name end) as customer, (case when sped.amount>0 then 'Deduction' end) as keterangan, 
(case when sped.amount>0 then pd.name end), (case when sped.amount>0 then round(sped.amount, 2) end) as total 
from c_bpartner bp
inner join pyr_sp_employee spe on spe.c_bpartner_id = bp.c_bpartner_id
inner join pyr_spe_deduction sped on  sped.pyr_sp_employee_id = spe.pyr_sp_employee_id
inner join pyr_deduction pd on pd.pyr_deduction_id=sped.pyr_deduction_id
where bp.isemployee = 'Y'
and spe.pyr_salarypayment_id=$P{pyr_salarypayment_id}
order by sped.amount desc) b
where b.* is not null]]></queryString>

		<field name="total_amount" class="java.math.BigDecimal"/>
		<field name="customer" class="java.lang.String"/>
		<field name="keterangan" class="java.lang.String"/>
		<field name="name" class="java.lang.String"/>
		<field name="total" class="java.math.BigDecimal"/>

	</subDataset>

	<parameter name="pyr_salarypayment_id" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[select sal.name, sal.description, sal.startdate, sal.enddate, sal.valutadate, sal.dateacct, sal.pyr_salarypayment_id,
bp.name as payroll, cr.iso_code, (case when sal.ismonthly='Y' then 'Yes' else 'No' end) as ismonthly, 
(case when sal.isbonus='Y' then 'Yes' else 'No' end) as isbonus, (case when sal.isperformanceinsentive= 'Y' then 'Yes' else 'No' end) as isperformanceinsentive,
(case when sal.isotherinsentive='Y' then 'Yes' else 'No' end) as isotherinsentive, (case when sal.isperformancedisinsentive='Y' then 'Yes' else 'No' end) as isperformancedisinsentive, 
(case when sal.isotherdisinsentive='Y' then 'Yes' else 'No' end) as isotherdisinsentive, sal.ad_client_id
from pyr_salarypayment sal
left join c_bpartner bp on bp.c_bpartner_id = sal.payrollmaster_id
left join c_currency cr on cr.c_currency_id = sal.c_currency_id
where sal.pyr_salarypayment_id=$P{pyr_salarypayment_id}]]></queryString>

