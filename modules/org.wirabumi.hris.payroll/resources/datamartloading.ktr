  <step>
    <name>read salary payment</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>openbravo</connection>
    <sql>select X.salarypayment as &#x22;Pembayaran Gaji&#x22;, X.site as &#x22;Site&#x22;, X.group as &#x22;Group&#x22;, X.karyawan, X.komponen as &#x22;Komponen Gaji&#x22;, X.jumlah as &#x22;Jumlah&#x22;from&#xa;&#x28;&#x28;select sp.name as salarypayment, eg.name as group, s.name as site, bp.value&#x7c;&#x7c;&#x27; - &#x27;&#x7c;&#x7c;bp.name as karyawan, e.name as komponen, sum&#x28;spee.amount&#x29; as jumlah&#xa;from pyr_salarypayment sp&#xa;left join pyr_sp_employee spe on spe.pyr_salarypayment_id&#x3d;sp.pyr_salarypayment_id&#xa;left join c_bpartner bp on bp.c_bpartner_id&#x3d;spe.c_bpartner_id&#xa;left join hris_site s on s.hris_site_id&#x3d;bp.em_hris_site&#xa;left join pyr_spe_earning spee on spee.pyr_sp_employee_id&#x3d;spe.pyr_sp_employee_id&#xa;left join pyr_earning e on e.pyr_earning_id&#x3d;spee.pyr_earning_id&#xa;left join pyr_earninggroup eg on eg.pyr_earninggroup_id&#x3d;e.pyr_earninggroup_id&#xa;group by sp.name, eg.name, s.name, bp.value, bp.name, e.name&#x29;&#xa;union all&#xa;&#x28;select sp.name as salarypayment, eg.name as group, s.name as site, bp.value&#x7c;&#x7c;&#x27; - &#x27;&#x7c;&#x7c;bp.name as karyawan, e.name as komponen, sum&#x28;spee.amount&#x29; as jumlah&#xa;from pyr_salarypayment sp&#xa;left join pyr_sp_employee spe on spe.pyr_salarypayment_id&#x3d;sp.pyr_salarypayment_id&#xa;left join c_bpartner bp on bp.c_bpartner_id&#x3d;spe.c_bpartner_id&#xa;left join hris_site s on s.hris_site_id&#x3d;bp.em_hris_site&#xa;left join pyr_spe_deduction spee on spee.pyr_sp_employee_id&#x3d;spe.pyr_sp_employee_id&#xa;left join pyr_deduction e on e.pyr_deduction_id&#x3d;spee.pyr_deduction_id&#xa;left join pyr_deductiongroup eg on eg.pyr_deductiongroup_id&#x3d;e.pyr_deductiongroup_id&#xa;group by sp.name,eg.name, s.name, bp.value, bp.name, e.name&#x29;&#xa;union all&#xa;&#x28;SELECT A.salarypayment, A.group, A.site, A.karyawan,&#xa;   unnest&#x28;array&#x5b;&#x27;bruto&#x27;, &#x27;biayajabatan&#x27;, &#x27;ptkp&#x27;, &#x27;pkp&#x27;, &#x27;pph21terhutang&#x27;, &#x27;pph21disetor&#x27;&#x5d;&#x29; AS komponen,&#xa;   unnest&#x28;array&#x5b;A.bruto, A.biayajabatan, A.ptkp, A.pkp, A.pph21terhutang, A.pph21disetor&#x5d;&#x29; AS jumlah&#xa;FROM &#x28;select	sp.name as salarypayment, &#x27;E.PERHITUNGAN PAJAK&#x27;&#x3a;&#x3a;varchar as group, s.name as site,&#xa;		bp.value&#x7c;&#x7c;&#x27; - &#x27;&#x7c;&#x7c;bp.name as karyawan, &#xa;		sum&#x28;pph.brutosetahungross&#x29; as bruto, sum&#x28;pph.biayajabatansetahun&#x29; as biayajabatan, sum&#x28;pph.ptkp&#x29;as ptkp, sum&#x28;pph.pkpsetahun&#x29; as pkp,&#xa;		sum&#x28;pph.pph21terhutang&#x29; as pph21terhutang, sum&#x28;pph.pph21terpotong&#x29; as pph21disetor&#xa;from pph_pph21 pph&#xa;left join pyr_sp_employee spe on spe.pyr_sp_employee_id&#x3d;pph.pyr_sp_employee_id&#xa;left join pyr_salarypayment sp on sp.pyr_salarypayment_id&#x3d;spe.pyr_salarypayment_id&#xa;left join c_bpartner bp on bp.c_bpartner_id&#x3d;pph.c_bpartner_id&#xa;left join hris_site s on s.hris_site_id&#x3d;bp.em_hris_site&#xa;&#xa;group by sp.name, s.name, bp.value, bp.name&#x29; A&#xa;ORDER BY jumlah&#x29;&#xa;union all&#xa;&#x28;select sp.name as salarypayment, &#x27;F. TAKE HOME PAY&#x27;&#x3a;&#x3a;varchar as group, s.name site, bp.value&#x7c;&#x7c;&#x27; - &#x27;&#x7c;&#x7c;bp.name as karyawan, &#x27;Take Home Pay&#x27;&#x3a;&#x3a;varchar as komponen, sum&#x28;spe.amount&#x29; as jumlah&#xa;from pyr_sp_employee spe&#xa;left join pyr_salarypayment sp on sp.pyr_salarypayment_id&#x3d;spe.pyr_salarypayment_id&#xa;left join c_bpartner bp on bp.c_bpartner_id&#x3d;spe.c_bpartner_id&#xa;left join hris_site s on s.hris_site_id&#x3d;bp.em_hris_site&#xa;group by sp.name, s.name, bp.value, bp.name&#x29;&#x29; X&#xa;order by X.salarypayment, X.site, X.group, X.karyawan, X.komponen</sql>
    <limit>0</limit>
    <lookup/>
